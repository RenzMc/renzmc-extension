{
  "name": "renzmc-language-support",
  "displayName": "RenzmcLang Language Support",
  "description": "Syntax highlighting and language support for RenzmcLang programming language",
  "version": "1.0.0",
  "publisher": "RenzMc",
  "author": {
    "name": "RenzMc",
    "email": "renzaja11@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RenzMc/renzmc-extension"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "renzmc",
    "renzmclang",
    "indonesian",
    "bahasa indonesia",
    "programming language"
  ],
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "renzmc",
        "aliases": [
          "RenzmcLang",
          "renzmc",
          "RMC"
        ],
        "extensions": [
          ".rmc",
          ".renzmc"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "renzmc",
        "scopeName": "source.renzmc",
        "path": "./syntaxes/renzmc.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "renzmc-file-icons",
        "label": "RenzmcLang File Icons",
        "path": "./fileicons/renzmc-icon-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0"
  }
}